div class="btn-group">
  <%= link_to "Back", root_path, class: "btn btn-default" %>
  <% if user_signed_in? %>
     <%= link_to "Edit", edit_old_path(@old), class: "btn btn-default" %>
     <%= link_to "Delete", old_path(@old), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-default" %>
  <% end %>
</div>
